<% provide(:title, 'Edit editora') %>
<h1>Edit editora</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@editora) do |f| %>
      <%= render "fields", f: f %>
    
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
      <%= link_to "Cancel", editoras_path, class: "btn btn-mini" %>
    <% end %>

  </div>
</div>