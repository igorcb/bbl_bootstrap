<% provide(:title, 'New assunto') %>
<h1>New assunto</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@assunto) do |f| %>
      <%= render 'fields', f: f %>
  
      <div class="form-actions">
	      <%= f.submit "Save assunto", class: "btn btn-primary" %>
	      <%= link_to "Cancel", assuntos_path, class: "btn btn-mini" %>
      </div>
  
    <% end %>
  </div>
</div>