<%= provide(:title, @assunto.descricao) %>
<p>
	<strong>Descrição:</strong>
	<%= @assunto.descricao %>
</p>

<div class="form-actions">
  <%= link_to t('views.new'), new_assunto_path, class: "btn btn-primary"  %>
  <%= link_to t('views.edit'), edit_assunto_path(@assunto), class: "btn"  %>
  <%= link_to t('views.list'), assuntos_path, class: "btn" %>
</div>
